<?xml version='1.0' encoding='UTF-8'?>
<package uuid="exploration-manager" version="0.0.4">
 <names>
  <name lang="en_US">noDrift</name>
  <name lang="en_US-x-scrald">Places</name>
 </names>
 <descriptions>
  <description lang="en_US">The robot explores its environement and makes a map.</description>
  <description lang="en_US-x-scrald">The robot explores its environement and makes a map.</description>
 </descriptions>
 <supportedLanguages>
  <language>en_US</language>
 </supportedLanguages>
 <descriptionLanguages>
  <language>en_US</language>
 </descriptionLanguages>
 <contents>
  <behaviorContent path="display_places">
   <userRequestable/>
   <nature>interactive</nature>
   <names>
    <name lang="en_US">display_places</name>
    <name lang="en_US-x-scrald">display_places</name>
   </names>
   <triggerSentences>
    <sentence lang="en_US">drift</sentence>
    <sentence lang="en_US">don't drift</sentence>
    <sentence lang="en_US">no drift</sentence>
    <sentence lang="en_US-x-scrald">drift</sentence>
    <sentence lang="en_US-x-scrald">don't drift</sentence>
    <sentence lang="en_US-x-scrald">no drift</sentence>
   </triggerSentences>
   <loadingResponses>
    <sentence lang="en_US">Let me show you the explorations</sentence>
    <sentence lang="en_US-x-scrald">Let me show you the explorations</sentence>
   </loadingResponses>
   <permissions>
    <canRunOnPod/>
   </permissions>
  </behaviorContent>
 </contents>
 <requirements>
  <naoqiRequirement minVersion="2.5.2"/>
  <robotRequirement model="JULIETTE_Y20"/>
 </requirements>
 <services>
  <service execStart="python noDriftManager.py" autorun="true" name="noDriftManager"/>
 </services>
</package>
